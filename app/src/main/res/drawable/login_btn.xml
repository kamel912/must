<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#449def" />
            <stroke android:width="1dp" android:color="#233c70" />
            <corners android:radius="3dp" />
            <padding android:left="4dp" android:top="10dp" android:right="4dp" android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#449def" android:endColor="#064a8a" android:angle="270.0" />
            <stroke android:width="1dp" android:color="#233c70" />
            <corners android:radius="3dp" />
            <padding android:left="4dp" android:top="10dp" android:right="4dp" android:bottom="10dp" />
        </shape>
    </item>
</selector>
